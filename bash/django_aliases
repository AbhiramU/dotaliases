# Run server
alias djrs='python manage.py runserver 0.0.0.0:8000'

# Run shell
alias djs='python manage.py shell'

# Run tests
alias djt='python manage.py test'

# Create super user
alias djsu='python manage.py createsuperuser'

# Load fixture
alias djl='python manage.py loaddata'

# Django Migrations
alias djmkmig='python manage.py makemigrations'
alias djmig='python manage.py migrate'
alias djdatamig='python manage.py makemigrations --empty'
